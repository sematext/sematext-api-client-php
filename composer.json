{
    "name": "sematext/stcloud",
    "version": "0.4.0",
    "description": "API client for talking to Sematext Cloud.",
    "keywords": [
        "logging",
        "monitoring",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://sematext.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sematext",
            "homepage": "https://sematext.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "friendsofphp/php-cs-fixer": "~2.12",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": { "sematext\\stcloud\\" : "stcloud/" }
    },
    "autoload-dev": {
        "psr-4": { "sematext\\stcloud\\" : "test/" }
    }
}
